{{outlet}}

<div class="content has-text-centered is-large">
  <h1>Most Popular</h1>
  <p>Check out the top 10 most popular songs on etch right now!</p>
</div>

  <div class="memories">

    {{#each model as |track index|}}
    <div class="memory">
      <div class="record record--with-center">
        <img class="record__pic" src="{{track.album.images.firstObject.url}}"/>
      </div>
      <div class="memory__content has-text-centered">
        <h2 class="title is-5">#{{index}} {{track.artists.[0].name}}</h2>
        <h2 class="subtitle is-6">{{track.name}}</h2>
      </div>
    </div>
    {{/each}}

  </div>
